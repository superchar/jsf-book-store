<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee 
    http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <from-view-id>/pages/user/authorization.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>successfulAuthorization</from-outcome>
            <to-view-id>/pages/user/successfulAuthorization.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>registration</from-outcome>
            <to-view-id>/pages/user/registration.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/pages/book/list.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>editing</from-outcome>
            <to-view-id>/pages/book/edit.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>adding</from-outcome>
            <to-view-id>/pages/book/addnew.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/pages/book/addnew.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>list</from-outcome>
            <to-view-id>/pages/book/list.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>